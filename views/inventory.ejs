								</section>

							<!-- Menu -->
								<nav id="menu">
									<header class="major">
										<h2>7419 Inventory System</h2>
									</header>
									<ul>
										<li><a href="index.html">Dashboard</a></li>
										<li><a href="inventory.html">Inventory</a></li>
										
									</ul>
								</nav>
